#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/../../../../../jest-cli@22.4.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/../../../../../jest-cli@22.4.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest-cli@22.4.4/node_modules/jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest-cli@22.4.4/node_modules/jest-cli/bin/jest.js" "$@"
fi
